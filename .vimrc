execute pathogen#infect()
syntax on
colorscheme solarized
let g:solarized_termcolors=256
let g:ctrlp_custom_ignore='\v[\/](node_modules|dist)|(\.(swp|ico|git))$'
set background=dark
set expandtab shiftwidth=2
set smarttab
set autoindent
set showcmd
set number
set ignorecase
set smartcase
set incsearch
set hlsearch
